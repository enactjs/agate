{
  "name": "@enact/agate",
  "version": "1.0.0-alpha.15",
  "description": "A theme library for Enact, containing a variety of UI components.",
  "repository": "https://github.com/enactjs/agate",
  "main": "index.js",
  "scripts": {
    "bootstrap": "npm link && enact link && cd samples && cd sampler && npm run bootstrap",
    "clean": "enact clean",
    "lint": "enact lint --local",
    "sampler": "npm run --prefix samples/sampler serve",
    "test": "enact test",
    "test-ui": "wdio tests/ui/wdio.conf.js",
    "test-ui-docker": "wdio tests/ui/wdio.docker.conf.js",
    "test-ui-tv": "wdio tests/ui/wdio.tv.conf.js",
    "test-watch": "enact test --watch",
    "transpile": "enact transpile"
  },
  "license": "Apache-2.0",
  "keywords": [
    "framework",
    "toolkit",
    "components",
    "mobile",
    "webOS"
  ],
  "enact": {
    "template": "html-template.ejs"
  },
  "eslintConfig": {
    "extends": "enact/strict",
    "root": true
  },
  "dependencies": {
    "@enact/core": "^3.2.0",
    "@enact/i18n": "^3.2.0",
    "@enact/spotlight": "^3.2.0",
    "@enact/ui": "^3.2.0",
    "classnames": "^2.2.6",
    "color-convert": "^1.9.3",
    "deep-freeze": "0.0.1",
    "eases": "^1.0.8",
    "immer": "^3.2.0",
    "prop-types": "^15.6.2",
    "ramda": "^0.25.0",
    "react": "^16.8.0",
    "react-dom": "^16.8.0",
    "recompose": "^0.30.0"
  },
  "peerDependencies": {
    "ilib": "^14.4.0 || ^14.4.0-webostv1"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.4.3",
    "@enact/ui-test-utils": "^0.1.0",
    "ilib": "^14.4.0"
  }
}
