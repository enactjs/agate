{
  "name": "agate-sampler",
  "version": "3.0.0",
  "description": "Component and QA samples for Agate",
  "private": true,
  "main": "index.js",
  "scripts": {
    "bootstrap": "enact bootstrap",
    "clean": "enact clean",
    "cleanall": "enact clean --all",
    "lint": "enact lint --strict",
    "test": "echo \"No test suite\" && exit 0",
    "pack": "storybook build -c .storybook -o dist",
    "pack-qa": "storybook build -c .qa-storybook -o dist",
    "serve": "storybook dev -p 8080 -c .storybook --ci",
    "serve-qa": "storybook dev -p 8080 -c .qa-storybook --ci"
  },
  "license": "Apache-2.0",
  "enact": {
    "theme": "agate"
  },
  "dependencies": {
    "@enact/agate": "../../",
    "@enact/core": "^5.0.0",
    "@enact/i18n": "^5.0.0",
    "@enact/spotlight": "^5.0.0",
    "@enact/storybook-utils": "^6.0.0",
    "@enact/ui": "^5.0.0",
    "@storybook/addon-docs": "^8.6.14",
    "@storybook/csf-tools": "^8.6.14",
    "@storybook/manager-api": "^8.6.14",
    "@storybook/react-webpack5": "^8.6.14",
    "@storybook/theming": "^8.6.14",
    "dotenv": "^16.5.0",
    "ilib": "^14.21.1",
    "prop-types": "^15.8.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "storybook": "^8.6.12"
  }
}
