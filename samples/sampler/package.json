{
  "name": "agate-sampler",
  "version": "1.1.0",
  "description": "Component and QA samples for Agate",
  "private": true,
  "main": "index.js",
  "scripts": {
    "bootstrap": "enact bootstrap",
    "clean": "enact clean",
    "cleanall": "enact clean --all",
    "lint": "enact lint --strict",
    "test": "echo \"No test suite\" && exit 0",
    "pack": "build-storybook -c .storybook -o dist",
    "pack-qa": "build-storybook -c .qa-storybook -o dist",
    "serve": "start-storybook -p 8080 -c .storybook --ci",
    "serve-qa": "start-storybook -p 8080 -c .qa-storybook --ci"
  },
  "license": "Apache-2.0",
  "enact": {
    "theme": "agate"
  },
  "dependencies": {
    "@enact/agate": "../../",
    "@enact/core": "^4.0.3",
    "@enact/i18n": "^4.0.3",
    "@enact/spotlight": "^4.0.3",
    "@enact/ui": "^4.0.3",
    "copy-to-clipboard": "^3.3.1",
    "dotenv": "^8.2.0",
    "ilib": "^14.4.0",
    "prop-types": "^15.7.2",
    "react": "^17.0.1",
    "storybook-color-picker": "^2.3.0"
  },
  "devDependencies": {
    "@enact/storybook-utils": "^4.1.0",
    "@storybook/addons": "6.3.12",
    "@storybook/react": "6.3.12",
    "@storybook/theming": "6.3.12",
    "babel-loader": "^8.2.2",
    "core-js": "^3.8.1"
  }
}
